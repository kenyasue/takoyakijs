{
    "compilerOptions": {
        "target": "ES2015",
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowJs": true,
        "sourceMap": true,
        "outDir": "dist",
        "downlevelIteration": true,
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "strictNullChecks": false,
        "skipLibCheck": true,
        "rootDir": "./"
    },
    "include": ["./**/*.ts"]
}
